# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org
root = true

[*]
indent_style = space
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.cs]
indent_size = 4
continuation_indent_size = 4

[*.js, *.html]
indent_size = 2
continuation_indent_size = 2

